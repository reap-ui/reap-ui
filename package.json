{
    "name": "reap-ui",
    "version": "1.0.0",
    "description": "Bootstrap components built with react",
    "main": "lib/index.js",
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "build:es": "tsc -d --outDir ./es",
        "build:lib": "cross-env BABEL_ENV=lib babel components --out-dir lib --extensions .ts,.tsx",
        "build:dev": "rollup -c",
        "build:prod": "cross-env NODE_ENV=production rollup -c",
        "build:doc": "webpack",
        "start": "webpack-dev-server --env development"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xroah/reap-ui.git"
    },
    "keywords": [
        "reap",
        "react",
        "bootstrap",
        "ui",
        "component",
        "js"
    ],
    "author": "xroah",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xroah/reap-ui/issues"
    },
    "homepage": "https://github.com/xroah/reap-ui#readme",
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/preset-typescript": "^7.10.1",
        "@rollup/plugin-babel": "^5.0.2",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-typescript": "^4.1.2",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^25.2.3",
        "@types/prop-types": "^15.7.3",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-syntax-highlighter": "^11.0.4",
        "@types/sinon": "^7.5.2",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.5.3",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.20.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^25.5.4",
        "mini-css-extract-plugin": "^0.8.2",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "raw-loader": "^3.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "react-syntax-highlighter": "^11.0.2",
        "redux": "^4.0.5",
        "rollup": "^1.32.1",
        "rollup-plugin-uglify": "^6.0.4",
        "sass-loader": "^8.0.2",
        "sinon": "^7.5.0",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^2.3.6",
        "typescript": "^3.9.3",
        "url-loader": "^3.0.0",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.10.2",
        "prop-types": "^15.7.2",
        "warning": "^4.0.3"
    },
    "files": [
        "dist",
        "lib",
        "es"
    ],
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/tests/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/tests/**/*.spec.[jt]s?(x)"
        ]
    }
}
