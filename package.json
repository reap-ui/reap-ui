{
    "name": "reap-ui",
    "version": "1.2.0",
    "description": "Bootstrap components built with react",
    "main": "lib/index.js",
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "lib": "tsc -m Commonjs -d --outDir ./lib",
        "es": "tsc -m ES6 -d --outDir ./es",
        "rollup": "rollup -c",
        "build:doc": "webpack",
        "build": "node ./rmdir && npm run lib && npm run es && npm run rollup",
        "start": "webpack-dev-server --env development",
        "lint": "eslint --ext .ts,.tsx components/",
        "fix": "eslint --ext .ts,.tsx --fix components/",
        "lint:doc": "eslint --ext .ts,.tsx docs/",
        "fix:doc": "eslint --ext .ts,.tsx --fix docs/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xroah/reap-ui.git"
    },
    "keywords": [
        "reap",
        "react",
        "bootstrap",
        "ui",
        "component",
        "js"
    ],
    "author": "xroah",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xroah/reap-ui/issues"
    },
    "homepage": "https://github.com/xroah/reap-ui#readme",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@rollup/plugin-babel": "^5.2.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-typescript": "^6.0.0",
        "@types/enzyme": "^3.10.7",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.14",
        "@types/prop-types": "^15.7.3",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-syntax-highlighter": "^11.0.5",
        "@types/sinon": "^9.0.6",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.5.2",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "eslint": "^7.9.0",
        "eslint-plugin-react": "^7.21.2",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.4.2",
        "mini-css-extract-plugin": "^0.11.2",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "raw-loader": "^4.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "react-syntax-highlighter": "^13.5.3",
        "redux": "^4.0.5",
        "rollup": "^2.28.2",
        "rollup-plugin-terser": "^7.0.2",
        "sass-loader": "^10.0.2",
        "sinon": "^9.0.3",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^4.2.2",
        "tslib": "^2.0.1",
        "typescript": "^4.0.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.2",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.11.2",
        "prop-types": "^15.7.2",
        "warning": "^4.0.3"
    },
    "files": [
        "dist",
        "lib",
        "es"
    ],
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/tests/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/tests/**/*.spec.[jt]s?(x)"
        ]
    }
}
